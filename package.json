{
    "name": "@fremtind/jkl",
    "private": true,
    "version": "2.0.0-alpha.0",
    "type": "module",
    "scripts": {
        "clean": "git clean -xfd",
        "build": "lerna run build",
        "commit": "git-cz",
        "cosign": "git-cz -S",
        "typecheck": "tsc --noEmit",
        "linker": "node scripts/linker.js",
        "lint": "biome check --reporter=summary",
        "lint:fix": "biome check --write --reporter=summary",
        "lint:ci": "biome lint --reporter=github",
        "ci:test": "run-p test lint:ci typecheck",
        "ci:changed": "lerna changed --parseable --loglevel error",
        "test": "pnpm --filter \"@fremtind/jokul\" test",
        "version:check": "lerna changed -l -a",
        "version": "lernacleanchangelog '**' && git add **/CHANGELOG.md **/package.json package.json pnpm-lock.yaml",
        "release": "pnpm build && lerna publish --conventional-graduate",
        "release:alpha": "pnpm build && lerna publish --conventional-prerelease --no-git-tag-version --no-changelog --no-private --dist-tag alpha",
        "release-gh": "lerna publish from-package",
        "dev": "storybook dev -p 6007",
        "portal:dev": "pnpm --filter \"portal\" dev",
        "portal:storybook": "storybook dev -p 6007 --no-open",
        "dev:portal": "run-p portal:*",
        "start": "pnpm --filter \"portal\" start",
        "serve:server": "node server.js",
        "serve:portal": "pnpm --filter \"portal\" start",
        "serve": "run-p serve:*",
        "boot": "pnpm install && pnpm prepare && pnpm build",
        "reboot": "pnpm clean && pnpm boot",
        "integration:ci": "playwright test",
        "integration:interactive": "playwright test --ui",
        "integration:axe": "playwright test -g axe",
        "build-storybook": "storybook build",
        "prepare": "husky || true"
    },
    "packageManager": "pnpm@7.32.2",
    "pnpm": {
        "overrides": {
            "@types/react": "18.3.1",
            "@types/react-dom": "18.3.1",
            "react": "18.3.1",
            "react-dom": "18.3.1",
            "react-is": "18.3.1",
            "axe-core": "^4.6.3",
            "chokidar": "^3.5.3",
            "cz-lerna-changelog>shelljs": "^0.8.5",
            "glob-stream>glob-parent": "^5.1.2",
            "nx": "15.4.2",
            "resolve-url-loader>loader-utils": "^1.4.2",
            "sharp": "0.33.5"
        },
        "patchedDependencies": {
            "cz-lerna-changelog@2.0.3": "patches/cz-lerna-changelog@2.0.3.patch"
        }
    },
    "dependencies": {
        "fs-extra": "^11.2.0",
        "glob": "^11.0.2",
        "nodemon": "^3.1.10",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-is": "^18.3.1"
    },
    "devDependencies": {
        "@axe-core/playwright": "^4.10.1",
        "@biomejs/biome": "1.9.4",
        "@changesets/cli": "^2.29.4",
        "@chromatic-com/storybook": "^3.2.4",
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@fremtind/browserslist-config-jkl": "^2.0.1",
        "@fremtind/stylelint-config-jkl": "workspace:*",
        "@playwright/test": "^1.52.0",
        "@storybook/addon-a11y": "^8.5.3",
        "@storybook/addon-essentials": "^8.5.3",
        "@storybook/addon-interactions": "^8.5.3",
        "@storybook/addon-onboarding": "^8.5.3",
        "@storybook/addon-storysource": "^8.6.7",
        "@storybook/blocks": "^8.5.3",
        "@storybook/nextjs": "^8.6.4",
        "@storybook/react": "^8.5.3",
        "@storybook/react-vite": "^8.5.3",
        "@storybook/test": "^8.5.3",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/express": "^5.0.2",
        "@types/glob": "^8.1.0",
        "@types/jest-axe": "^3.5.9",
        "@types/node": "^22.15.29",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.7",
        "@types/react-is": "^18.3.1",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@types/testing-library__jest-dom": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^8.33.0",
        "@typescript-eslint/parser": "^8.33.0",
        "@vitejs/plugin-react": "^4.5.0",
        "@vitejs/plugin-react-swc": "^3.10.0",
        "autoprefixer": "^10.4.21",
        "browserslist": "^4.25.0",
        "case": "^1.6.3",
        "clsx": "^2.1.1",
        "commitizen": "^4.3.1",
        "cssnano": "^7.0.7",
        "cssnano-preset-lite": "^4.0.4",
        "cz-lerna-changelog": "2.0.3",
        "esbuild": "^0.25.5",
        "esbuild-plugin-browserslist": "~1.0.1",
        "esbuild-sass-plugin": "^3.3.1",
        "express": "^5.1.0",
        "express-http-proxy": "^2.1.1",
        "get-port": "^7.1.0",
        "gh-pages": "6.3.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "jest-axe": "^10.0.0",
        "jest-canvas-mock": "^2.5.2",
        "jest-environment-jsdom": "30.0.0-beta.3",
        "jest-watch-typeahead": "^2.2.2",
        "lerna": "8.2.2",
        "lerna-clean-changelogs-cli": "4.0.9",
        "lint-staged": "^16.1.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.5.4",
        "prompt": "^1.3.0",
        "react-hook-form": "^7.57.0",
        "react-live": "^4.1.8",
        "react-syntax-highlighter": "^15.6.1",
        "replace-in-file": "^8.3.0",
        "resize-observer-polyfill": "^1.5.1",
        "rimraf": "^6.0.1",
        "sass": "^1.89.1",
        "storybook": "^8.6.7",
        "stylelint": "^16.20.0",
        "tiny-glob": "^0.2.9",
        "typescript": "^5.8.3",
        "vite": "^6.3.5",
        "vite-plugin-dts": "^4.5.4"
    },
    "browserslist": [
        ">0.2% in no",
        "not ie <= 11",
        "not dead",
        "not op_mini all"
    ],
    "lint-staged": {
        "*.{html,json,md,mdx,ts,tsx,js,jsx}": [
            "pnpm biome check --write --no-errors-on-unmatched"
        ],
        "*.{scss,css}": [
            "stylelint --fix --config=packages/stylelint-config-jkl/index.mjs"
        ]
    },
    "stylelint": {
        "extends": "./packages/stylelint-config-jkl/index.mjs"
    },
    "eslintConfig": {
        "extends": ["plugin:storybook/recommended"]
    }
}
