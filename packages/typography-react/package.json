{
    "name": "@fremtind/jkl-typography-react",
    "version": "1.0.8",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react typography components",
    "homepage": "https://fremtind.github.io/jokul",
    "keywords": [
        "typography",
        "heading",
        "paragraph",
        "small text",
        "lead text",
        "jøkul",
        "fremtind"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/browser/index.js",
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "rimraf build/ dist/ .cache node_modules/",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "rollup --config ../../rollup.config.js",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:style": "lerna exec --scope=@fremtind/jkl-core gulp build:watch",
        "dev:js": "parcel documentation/index.html",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-core": "^1.1.7"
    },
    "devDependencies": {
        "@fremtind/browserslist-config-jkl": "^0.4.7"
    },
    "peerDependencies": {
        "@types/react": "^16.8.17",
        "@types/react-dom": "^16.8.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    },
    "browserslist": [
        "extends @fremtind/browserslist-config-jkl"
    ],
    "gitHead": "f3d59e91b918c1c3d586396a66868b843b79996e"
}
