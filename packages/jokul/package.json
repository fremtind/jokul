{
    "name": "@fremtind/jokul",
    "version": "0.23.3",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "description": "JÃ¸kul monopakke",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/jokul",
    "keywords": [],
    "license": "MIT",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "files": [
        "./build",
        "./styles"
    ],
    "exports": {
        ".": {
            "types": "./build/index.d.ts",
            "require": "./build/cjs/index.js",
            "import": "./build/es/index.js"
        },
        "./core": {
            "types": "./build/core/index.d.ts",
            "require": "./build/cjs/core/index.js",
            "import": "./build/es/core/index.js"
        },
        "./core/tokens.less": "./src/core/tokens.less",
        "./core/jkl": "./src/core/jkl/_index.scss",
        "./hooks": {
            "types": "./build/hooks/index.d.ts",
            "require": "./build/cjs/hooks/index.js",
            "import": "./build/es/hooks/index.js"
        },
        "./utilities": {
            "types": "./build/utilities/index.d.ts",
            "require": "./build/cjs/utilities/index.js",
            "import": "./build/es/utilities/index.js"
        },
        "./components": {
            "types": "./build/components/index.d.ts",
            "require": "./build/cjs/components/index.js",
            "import": "./build/es/components/index.js"
        },
        "./styles": "./styles/*",
        "./styles/core/*": "./styles/core/*",
        "./styles/accordion/*": "./styles/components/accordion/*",
        "./styles/breadcrumb/*": "./styles/components/breadcrumb/*",
        "./styles/button/*": "./styles/components/button/*",
        "./styles/card/*": "./styles/components/card/*",
        "./styles/checkbox/*": "./styles/components/checkbox/*",
        "./styles/combobox/*": "./styles/components/combobox/*",
        "./styles/cookie-consent/*": "./styles/components/cookie-consent/*",
        "./styles/datepicker/*": "./styles/components/datepicker/*",
        "./styles/feedback/*": "./styles/components/feedback/*",
        "./styles/description-list/*": "./styles/components/description-list/*",
        "./styles/expander/*": "./styles/components/expander/*",
        "./styles/icon/*": "./styles/components/icon/*",
        "./styles/icon-button/*": "./styles/components/icon-button/*",
        "./styles/input-group/*": "./styles/components/input-group/*",
        "./styles/image/*": "./styles/components/image/*",
        "./styles/link/*": "./styles/components/link/*",
        "./styles/link-list/*": "./styles/components/link-list/*",
        "./styles/list/*": "./styles/components/list/*",
        "./styles/loader/*": "./styles/components/loader/*",
        "./styles/logo/*": "./styles/components/logo/*",
        "./styles/menu/*": "./styles/components/menu/*",
        "./styles/message/*": "./styles/components/message/*",
        "./styles/modal/*": "./styles/components/modal/*",
        "./styles/pagination/*": "./styles/components/pagination/*",
        "./styles/popover/*": "./styles/components/popover/*",
        "./styles/progress-bar/*": "./styles/components/progress-bar/*",
        "./styles/radio-button/*": "./styles/components/radio-button/*",
        "./styles/select/*": "./styles/components/select/*",
        "./styles/summary-table/*": "./styles/components/summary-table/*",
        "./styles/system-message/*": "./styles/components/system-message/*",
        "./styles/table/*": "./styles/components/table/*",
        "./styles/tabs/*": "./styles/components/tabs/*",
        "./styles/tag/*": "./styles/components/tag/*",
        "./styles/text-input/*": "./styles/components/text-input/*",
        "./styles/toast/*": "./styles/components/toast/*",
        "./styles/toggle-switch/*": "./styles/components/toggle-switch/*",
        "./styles/tooltip/*": "./styles/components/tooltip/*"
    },
    "scripts": {
        "generate:tokens": "node tokens.build.mjs",
        "build": "pnpm run /compile:*/",
        "dev": "babel --out-dir=. dev-server.mjs && node dev-server.js",
        "compile:style": "node build-styles.mjs",
        "compile:scripts": "vite build --config vite.build.config.mjs"
    },
    "peerDependencies": {
        "react": "^18.0.0"
    },
    "optionalDependencies": {
        "@floating-ui/react": ">0.24.1 <0.27",
        "@react-aria/toast": "^3.0.0-beta.1",
        "@react-stately/toast": "^3.0.0-beta.0",
        "date-fns": "^2.30.0",
        "framer-motion": ">7.10.2 <12",
        "react-a11y-dialog": "^6.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.25.6",
        "@babel/preset-react": "^7.24.7",
        "@rollup/plugin-terser": "^0.4.4",
        "@vitejs/plugin-react-swc": "^3.7.0",
        "autoprefixer": "^10.4.14",
        "change-case": "^4.1.2",
        "clsx": "^2.1.1",
        "cssnano": "^7.0.6",
        "cssnano-preset-lite": "^4.0.3",
        "glob": "^11.0.0",
        "ink": "^5.0.1",
        "ink-select-input": "^6.0.0",
        "nanoid": "^3.3.6",
        "postcss": "^8.4.24",
        "react-hook-form": "^7.44.3",
        "rollup-plugin-node-externals": "^7.1.3",
        "rollup-plugin-visualizer": "^5.12.0",
        "sass-embedded": "^1.78.0",
        "style-dictionary": "^3.8.0",
        "vite": "^5.2.0",
        "vite-plugin-dts": "^4.1.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    },
    "babel": {
        "presets": [
            "@babel/preset-react"
        ]
    }
}
