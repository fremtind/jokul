{
    "name": "@fremtind/jokul",
    "version": "0.21.2",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "description": "JÃ¸kul monopakke",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/jokul",
    "keywords": [],
    "license": "MIT",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "files": [
        "./build",
        "./src/core/styles",
        "./src/core/jkl",
        "./src/core/tokens.less",
        "./src/components/accordion/styles",
        "./src/components/breadcrumb/styles",
        "./src/components/button/styles",
        "./src/components/card/styles",
        "./src/components/checkbox/styles",
        "./src/components/combobox/styles",
        "./src/components/cookie-consent/styles",
        "./src/components/datepicker/styles",
        "./src/components/feedback/styles",
        "./src/components/description-list/styles",
        "./src/components/expander/styles",
        "./src/components/icon/styles",
        "./src/components/icon-button/styles",
        "./src/components/input-group/styles",
        "./src/components/image/styles",
        "./src/components/link/styles",
        "./src/components/link-list/styles",
        "./src/components/list/styles",
        "./src/components/loader/styles",
        "./src/components/logo/styles",
        "./src/components/menu/styles",
        "./src/components/message/styles",
        "./src/components/modal/styles",
        "./src/components/popover/styles",
        "./src/components/progress-bar/styles",
        "./src/components/radio-button/styles",
        "./src/components/select/styles",
        "./src/components/summary-table/styles",
        "./src/components/system-message/styles",
        "./src/components/table/styles",
        "./src/components/tabs/styles",
        "./src/components/tag/styles",
        "./src/components/text-input/styles",
        "./src/components/toast/styles",
        "./src/components/toggle-switch/styles",
        "./src/components/tooltip/styles",
        "./src/styles/*"
    ],
    "exports": {
        ".": {
            "types": "./build/index.d.ts",
            "require": "./build/cjs/index.js",
            "import": "./build/es/index.js"
        },
        "./core": {
            "types": "./build/core/index.d.ts",
            "require": "./build/cjs/core/index.js",
            "import": "./build/es/core/index.js"
        },
        "./core/tokens.less": "./src/core/tokens.less",
        "./core/jkl": "./src/core/jkl/_index.scss",
        "./hooks": {
            "types": "./build/hooks/index.d.ts",
            "require": "./build/cjs/hooks/index.js",
            "import": "./build/es/hooks/index.js"
        },
        "./utilities": {
            "types": "./build/utilities/index.d.ts",
            "require": "./build/cjs/utilities/index.js",
            "import": "./build/es/utilities/index.js"
        },
        "./components": {
            "types": "./build/components/index.d.ts",
            "require": "./build/cjs/components/index.js",
            "import": "./build/es/components/index.js"
        },
        "./styles": "./src/styles/*",
        "./styles/core/*": "./src/core/styles/*",
        "./styles/accordion/*": "./src/components/accordion/styles/*",
        "./styles/breadcrumb/*": "./src/components/breadcrumb/styles/*",
        "./styles/button/*": "./src/components/button/styles/*",
        "./styles/card/*": "./src/components/card/styles/*",
        "./styles/checkbox/*": "./src/components/checkbox/styles/*",
        "./styles/combobox/*": "./src/components/combobox/styles/*",
        "./styles/cookie-consent/*": "./src/components/cookie-consent/styles/*",
        "./styles/datepicker/*": "./src/components/datepicker/styles/*",
        "./styles/feedback/*": "./src/components/feedback/styles/*",
        "./styles/description-list/*": "./src/components/description-list/styles/*",
        "./styles/expander/*": "./src/components/expander/styles/*",
        "./styles/icon/*": "./src/components/icon/styles/*",
        "./styles/icon-button/*": "./src/components/icon-button/styles/*",
        "./styles/input-group/*": "./src/components/input-group/styles/*",
        "./styles/image/*": "./src/components/image/styles/*",
        "./styles/link/*": "./src/components/link/styles/*",
        "./styles/link-list/*": "./src/components/link-list/styles/*",
        "./styles/list/*": "./src/components/list/styles/*",
        "./styles/loader/*": "./src/components/loader/styles/*",
        "./styles/logo/*": "./src/components/logo/styles/*",
        "./styles/menu/*": "./src/components/menu/styles/*",
        "./styles/message/*": "./src/components/message/styles/*",
        "./styles/modal/*": "./src/components/modal/styles/*",
        "./styles/popover/*": "./src/components/popover/styles/*",
        "./styles/progress-bar/*": "./src/components/progress-bar/styles/*",
        "./styles/radio-button/*": "./src/components/radio-button/styles/*",
        "./styles/select/*": "./src/components/select/styles/*",
        "./styles/summary-table/*": "./src/components/summary-table/styles/*",
        "./styles/system-message/*": "./src/components/system-message/styles/*",
        "./styles/table/*": "./src/components/table/styles/*",
        "./styles/tabs/*": "./src/components/tabs/styles/*",
        "./styles/tag/*": "./src/components/tag/styles/*",
        "./styles/text-input/*": "./src/components/text-input/styles/*",
        "./styles/toast/*": "./src/components/toast/styles/*",
        "./styles/toggle-switch/*": "./src/components/toggle-switch/styles/*",
        "./styles/tooltip/*": "./src/components/tooltip/styles/*"
    },
    "scripts": {
        "generate:tokens": "node tokens.build.mjs",
        "build": "pnpm run /compile:*/",
        "dev": "babel --out-dir=. dev-server.mjs && node dev-server.js",
        "compile:style": "node build-styles.mjs",
        "compile:scripts": "vite build --config vite.build.config.mjs"
    },
    "peerDependencies": {
        "react": "^18.0.0"
    },
    "optionalDependencies": {
        "@floating-ui/react": ">0.24.1 <0.27",
        "@react-aria/toast": "^3.0.0-beta.1",
        "@react-stately/toast": "^3.0.0-beta.0",
        "date-fns": "^2.30.0",
        "framer-motion": ">7.10.2 <12",
        "react-a11y-dialog": "^6.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.25.6",
        "@babel/preset-react": "^7.24.7",
        "@rollup/plugin-terser": "^0.4.4",
        "@vitejs/plugin-react-swc": "^3.7.0",
        "autoprefixer": "^10.4.14",
        "change-case": "^4.1.2",
        "clsx": "^2.1.1",
        "cssnano": "^7.0.6",
        "cssnano-preset-lite": "^4.0.3",
        "glob": "^11.0.0",
        "ink": "^5.0.1",
        "ink-select-input": "^6.0.0",
        "nanoid": "^3.3.6",
        "postcss": "^8.4.24",
        "react-hook-form": "^7.44.3",
        "rollup-plugin-node-externals": "^7.1.3",
        "rollup-plugin-visualizer": "^5.12.0",
        "sass-embedded": "^1.78.0",
        "style-dictionary": "^3.8.0",
        "vite": "^5.2.0",
        "vite-plugin-dts": "^4.1.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    },
    "babel": {
        "presets": [
            "@babel/preset-react"
        ]
    }
}
