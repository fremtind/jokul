{
    "name": "@fremtind/jkl-card-react",
    "version": "8.0.3",
    "publishConfig": {
        "access": "public"
    },
    "description": "JÃ¸kul react card component",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/card-react",
    "keywords": [
        "card"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "rimraf node_modules/ build/ node_modules/",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "node ./esbuild.prod.mjs",
        "build": "run-s build:*",
        "test": "jest --testMatch '**/card-react/**/*.test.+(ts|tsx|js)' --config=../../jest.config.js",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.dev.mjs",
        "dev:server": "node ./dist/server.js",
        "dev": "node ./esbuild.dev.mjs && run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-button-react": "^13.0.0",
        "@fremtind/jkl-card": "^8.0.0",
        "@fremtind/jkl-core": "^12.0.0",
        "@fremtind/jkl-image-react": "^5.0.0",
        "@fremtind/jkl-tag-react": "^4.0.1",
        "classnames": "^2.3.2"
    },
    "devDependencies": {
        "@fremtind/jkl-constants-util": "^2.0.2"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
