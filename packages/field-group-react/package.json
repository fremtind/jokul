{
    "name": "@fremtind/jkl-field-group-react",
    "version": "0.3.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "Used to create a fieldset around groups of inputs",
    "homepage": "https://fremtind.github.io/jokul",
    "keywords": [
        "form"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/browser/index.js",
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "rollup --config ../../rollup.config.js",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:style": "lerna exec --scope=@fremtind/jkl-field-group yarn build:watch",
        "dev:js": "parcel example/index.html",
        "dev": "run-p dev:*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "dependencies": {
        "@fremtind/jkl-field-group": "^0.3.0",
        "@fremtind/jkl-typography-react": "^0.9.0"
    },
    "devDependencies": {
        "@fremtind/browserslist-config-jkl": "^0.4.0",
        "@fremtind/jkl-core": "^0.11.0"
    },
    "peerDependencies": {
        "@types/react": "^16.8.17",
        "@types/react-dom": "^16.8.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
