{
    "name": "@fremtind/jkl-field-group-react",
    "version": "7.0.7",
    "publishConfig": {
        "access": "public"
    },
    "description": "Used to create a fieldset around groups of inputs",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/field-group-react",
    "keywords": [
        "form"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "rimraf .turbo/ build/ node_modules/",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "node ./esbuild.prod.mjs",
        "build": "run-s build:*",
        "test": "jest --testMatch '**/field-group-react/**/*.test.+(ts|tsx|js)' --config=../../jest.config.js",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.dev.mjs",
        "dev:server": "node ./dist/server.js",
        "predev": "node ./esbuild.dev.mjs",
        "dev": "run-p dev:*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "dependencies": {
        "@fremtind/jkl-field-group": "^6.0.3",
        "@fremtind/jkl-react-hooks": "^10.2.1",
        "classnames": "^2.2.6"
    },
    "devDependencies": {
        "@fremtind/jkl-core": "^11.1.0",
        "@fremtind/jkl-internal-doc-utils": "^1.0.0"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
