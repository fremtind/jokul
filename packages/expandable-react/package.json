{
    "name": "@fremtind/jkl-expandable-react",
    "version": "7.2.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react expandable",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/expandable-react",
    "keywords": [
        "jøkul",
        "fremtind"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "scripts": {
        "clean": "git clean -xfd",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "node ./esbuild.prod.mjs",
        "build": "run-s build:*",
        "test": "jest --testMatch '**/expandable-react/**/*.test.+(ts|tsx|js)' --config=../../jest.config.js",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.dev.mjs",
        "dev:server": "nodemon ./dist/server.js",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-core": "^14.8.9",
        "@fremtind/jkl-expandable": "^7.2.4",
        "@fremtind/jkl-icons-react": "^9.1.22",
        "@fremtind/jkl-react-hooks": "^12.2.13",
        "classnames": "^2.5.1"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    },
    "devDependencies": {
        "@fremtind/jkl-button-react": "^15.2.20",
        "@fremtind/jkl-text-input-react": "^16.0.7"
    }
}
