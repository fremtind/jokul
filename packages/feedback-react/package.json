{
    "name": "@fremtind/jkl-feedback-react",
    "version": "12.0.49",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react feedback component",
    "homepage": "https://jokul.fremtind.no",
    "keywords": [
        "feedback",
        "jøkul",
        "fremtind"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/browser/index.js",
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "rimraf .turbo/ build/ node_modules/",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "rollup --config ../../rollup.config.js",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:js": "parcel documentation/index.html",
        "dev:style": "lerna exec --scope=@fremtind/jkl-feedback yarn build:watch",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@babel/runtime": "^7.17.2",
        "@fremtind/jkl-button-react": "^10.1.3",
        "@fremtind/jkl-checkbox-react": "^6.1.6",
        "@fremtind/jkl-core": "^9.3.0",
        "@fremtind/jkl-feedback": "^9.0.7",
        "@fremtind/jkl-message-box-react": "^6.3.6",
        "@fremtind/jkl-radio-button-react": "^6.1.14",
        "@fremtind/jkl-react-hooks": "^8.0.1",
        "@fremtind/jkl-text-input-react": "^9.0.12",
        "@fremtind/jkl-validators-util": "^2.3.1",
        "classnames": "^2.2.6"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0",
        "react": "^16.8.6 || ^17.0.0",
        "react-dom": "^16.8.6 || ^17.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
