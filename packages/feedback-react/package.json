{
    "name": "@fremtind/jkl-feedback-react",
    "version": "18.2.7",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react feedback component",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/feedback-react",
    "keywords": [
        "feedback",
        "jøkul",
        "fremtind"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "git clean -xfd",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "node ./esbuild.prod.mjs",
        "build": "run-s build:*",
        "test": "jest --testMatch '**/feedback-react/**/*.test.+(ts|tsx|js)' --config=../../jest.config.js",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.dev.mjs",
        "dev:server": "nodemon ./dist/server.js",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-button-react": "^15.2.3",
        "@fremtind/jkl-checkbox-react": "^11.1.42",
        "@fremtind/jkl-core": "^14.8.0",
        "@fremtind/jkl-feedback": "^13.1.4",
        "@fremtind/jkl-input-group-react": "^3.0.49",
        "@fremtind/jkl-message-react": "^1.1.6",
        "@fremtind/jkl-radio-button-react": "^11.1.88",
        "@fremtind/jkl-react-hooks": "^12.2.4",
        "@fremtind/jkl-text-input-react": "^15.2.3",
        "@fremtind/jkl-validators-util": "^3.0.1",
        "classnames": "^2.3.2"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
