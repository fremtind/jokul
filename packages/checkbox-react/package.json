{
    "name": "@fremtind/jkl-checkbox-react",
    "version": "0.6.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react checkbox component",
    "homepage": "https://fremtind.github.io/jokul",
    "keywords": [
        "checkbox",
        "form",
        "jøkul",
        "fremtind"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/browser/index.js",
    "scripts": {
        "clean": "rimraf build/ dist/ .cache",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "rollup --config ../../rollup.config.js",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:style": "lerna exec --scope=@fremtind/jkl-checkbox yarn build:watch",
        "dev:js": "parcel example/index.html",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-checkbox": "^0.7.2",
        "@fremtind/jkl-core": "^0.8.0"
    },
    "devDependencies": {
        "@fremtind/browserslist-config-jkl": "^0.3.3",
        "npm-run-all": "^4.1.5"
    },
    "peerDependencies": {
        "@types/react": "^16.8.17",
        "@types/react-dom": "^16.8.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    },
    "browserslist": [
        "extends @fremtind/browserslist-config-jkl"
    ],
    "gitHead": "d2f6e412cb758cef989870d635fceab6c0367bfe"
}
