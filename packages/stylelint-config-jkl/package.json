{
    "name": "@fremtind/stylelint-config-jkl",
    "version": "3.0.3",
    "description": "Styleint configuration for Jøkul",
    "keywords": [
        "stylelint",
        "jøkul",
        "fremtind"
    ],
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/stylelint-config-jkl",
    "license": "MIT",
    "main": "index.js",
    "files": [
        "rules/"
    ],
    "scripts": {
        "clean": "rimraf .turbo/ node_modules/ .stylelintcache",
        "build": "npm run lint",
        "lint": "stylelint --config=index.js 'examples/**/*.{css,scss,sass}' --cache",
        "test": "npm run lint"
    },
    "dependencies": {
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^27.0.0",
        "stylelint-config-standard-scss": "^5.0.0"
    },
    "peerDependencies": {
        "stylelint": "^14.0.0"
    },
    "devDependencies": {
        "stylelint": "^14.8.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
