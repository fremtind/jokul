{
    "name": "@fremtind/jkl-accordion-react",
    "version": "12.1.2",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react accordion component",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/accordion-react",
    "keywords": [
        "accordion",
        "jøkul",
        "fremtind"
    ],
    "license": "MIT",
    "type": "module",
    "main": "./build/cjs/index.js",
    "module": "./build/es/index.js",
    "browser": "./build/es/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "types": "./build/index.d.ts",
    "exports": {
        ".": {
            "types": "./build/index.d.ts",
            "import": "./build/es/index.js",
            "require": "./build/cjs/index.js"
        },
        "./documentation": {
            "types": "./build/documentation/index.d.ts",
            "import": "./build/documentation/*"
        }
    },
    "scripts": {
        "clean": "rimraf build/ node_modules/",
        "build": "vite build --config vite.build.config.ts",
        "test": "jest --testMatch '**/accordion-react/**/*.test.+(ts|tsx|js)' --config=../../jest.config.js",
        "dev": "vite --config vite.dev.config.ts --port 3000"
    },
    "dependencies": {
        "@fremtind/jkl-accordion": "^12.0.1",
        "@fremtind/jkl-core": "^14.1.1",
        "@fremtind/jkl-icons-react": "^9.1.1",
        "@fremtind/jkl-react-hooks": "^12.1.3",
        "classnames": "^2.3.2"
    },
    "devDependencies": {
        "@fremtind/jkl-button": "^12.1.3",
        "@fremtind/jkl-expand-button": "^7.0.4",
        "@fremtind/jkl-icons": "^9.1.4",
        "@fremtind/jkl-list-react": "^10.2.2",
        "@rollup/plugin-terser": "^0.4.4",
        "rollup-plugin-node-externals": "^7.1.2",
        "rollup-plugin-visualizer": "^5.12.0",
        "vite": "^5.2.0"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
