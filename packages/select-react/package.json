{
    "name": "@fremtind/jkl-select-react",
    "version": "1.1.6",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react select component",
    "homepage": "https://fremtind.github.io/jokul",
    "keywords": [
        "dropdown",
        "select",
        "form",
        "jøkul",
        "fremtind"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/browser/index.js",
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "rimraf build/ dist/ .cache node_modules/",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "rollup --config ../../rollup.config.js",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:style": "lerna exec --scope=@fremtind/jkl-select yarn build:watch",
        "dev:js": "parcel example/index.html",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-core": "^1.1.6",
        "@fremtind/jkl-select": "^1.1.6",
        "@fremtind/jkl-typography-react": "^1.0.7",
        "@nrk/core-toggle": "^3.0.4",
        "nanoid": "^2.1.6"
    },
    "devDependencies": {
        "@fremtind/browserslist-config-jkl": "^0.4.6",
        "@types/nanoid": "^2.0.0"
    },
    "peerDependencies": {
        "@types/react": "^16.9.4",
        "@types/react-dom": "^16.8.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    },
    "browserslist": [
        "extends @fremtind/browserslist-config-jkl"
    ],
    "gitHead": "e161df7072d73fd2c3e28a935ffaeaca6d9ea6d8"
}
