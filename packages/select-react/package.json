{
    "name": "@fremtind/jkl-select-react",
    "version": "15.0.15",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react select component",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/select-react",
    "keywords": [
        "dropdown",
        "select",
        "form",
        "jøkul",
        "fremtind"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "git clean -xfd",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "node ./esbuild.prod.mjs",
        "build": "run-s build:*",
        "test": "jest --testMatch '**/select-react/**/*.test.+(ts|tsx|js)' --config=../../jest.config.js",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.dev.mjs",
        "dev:server": "nodemon ./dist/server.js",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-core": "^14.10.0",
        "@fremtind/jkl-icons-react": "^9.2.0",
        "@fremtind/jkl-input-group-react": "^4.0.7",
        "@fremtind/jkl-react-hooks": "^12.2.15",
        "@fremtind/jkl-select": "^11.1.36",
        "classnames": "^2.5.1"
    },
    "devDependencies": {
        "@fremtind/jkl-accordion-react": "^13.0.2",
        "@fremtind/jkl-tooltip-react": "^4.6.10",
        "react-hook-form": "^7.54.2"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
