{
    "name": "@fremtind/jkl-input-group-react",
    "version": "4.0.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react input-group",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/input-group-react",
    "keywords": [
        "jøkul",
        "fremtind"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "scripts": {
        "clean": "git clean -xfd",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "node ./esbuild.prod.mjs",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.dev.mjs",
        "dev:server": "nodemon ./dist/server.js",
        "predev": "node ./esbuild.dev.mjs",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-core": "^14.8.5",
        "@fremtind/jkl-icons-react": "^9.1.17",
        "@fremtind/jkl-input-group": "^3.1.9",
        "@fremtind/jkl-react-hooks": "^12.2.9",
        "classnames": "^2.3.2"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
