{
    "name": "@fremtind/jkl-toast-react",
    "version": "2.4.3",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react toast",
    "homepage": "https://github.com/fremtind/jokul/tree/pre-1.0/packages/toast-react",
    "keywords": [
        "jøkul",
        "fremtind"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "scripts": {
        "clean": "git clean -xfd",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "node ./esbuild.prod.mjs",
        "build": "run-s build:*",
        "test": "jest --testMatch '**/toast-react/**/*.test.+(ts|tsx|js)' --config=../../jest.config.js",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.dev.mjs",
        "dev:server": "nodemon ./dist/server.js",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-core": "^14.10.8",
        "@fremtind/jkl-icon-button-react": "^5.0.44",
        "@fremtind/jkl-icons-react": "^9.2.10",
        "@fremtind/jkl-progress-bar-react": "^6.1.23",
        "@fremtind/jkl-react-hooks": "^12.3.3",
        "@fremtind/jkl-toast": "^2.2.24",
        "@react-aria/toast": "3.0.0-beta.18",
        "@react-stately/toast": "3.0.0-beta.7",
        "classnames": "^2.5.1"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
