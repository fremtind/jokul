{
    "name": "@fremtind/jkl-datepicker-react",
    "version": "3.7.6",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react datepicker component",
    "homepage": "https://fremtind.github.io/jokul",
    "keywords": [
        "datepicker",
        "jøkul",
        "fremtind"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/browser/index.js",
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "rimraf node_modules/ .cache/ build/ dist/ public/ **/*.css",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "rollup --config ../../rollup.config.js",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:js": "parcel documentation/index.html",
        "dev:style": "lerna exec --scope=@fremtind/jkl-datepicker yarn build:watch",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@babel/runtime": "^7.9.0",
        "@fremtind/jkl-core": "^4.20.0",
        "@fremtind/jkl-datepicker": "^1.5.43",
        "@fremtind/jkl-icon-button-react": "^0.4.3",
        "@fremtind/jkl-react-hooks": "^1.10.2",
        "@fremtind/jkl-select-react": "^3.0.3",
        "@fremtind/jkl-text-input-react": "^3.8.2",
        "classnames": "^2.2.6",
        "nanoid": "^3.1.10"
    },
    "devDependencies": {
        "@fremtind/jkl-portal-components": "^0.9.2",
        "@nrk/core-datepicker": "3.0.8"
    },
    "peerDependencies": {
        "@types/react": "^16.9.4",
        "@types/react-dom": "^16.8.4",
        "react": "^16.8.6 || ^17.0.0",
        "react-dom": "^16.8.6 || ^17.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    },
    "gitHead": "ffcb2803b73ddd6f1dd37bb0eed345ca801bfebb"
}
