{
    "name": "@fremtind/jkl-autosuggest-react",
    "version": "1.0.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react autosuggest component",
    "keywords": [
        "autosuggest",
        "autocomplete",
        "suggest",
        "jøkul",
        "fremtind"
    ],
    "license": "MIT",
    "homepage": "https://fremtind.github.io/jokul",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/browser/index.js",
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "fix": "prettier --write '**/*.{js,jsx,ts,tsx}' '**/*.scss' '**/*.html'",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "rollup --config ../../rollup.config.js",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:style": "lerna exec --scope=@fremtind/jkl-autosuggest yarn build:watch",
        "dev:js": "parcel example/index.html",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-autosuggest": "^1.0.0",
        "@fremtind/jkl-typography-react": "^0.7.0",
        "@types/match-sorter": "^2.3.0",
        "downshift": "^3.2.12",
        "match-sorter": "^4.0.1"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5",
        "@fremtind/browserslist-config-jkl": "^0.3.3"
    },
    "peerDependencies": {
        "@types/react": "^16.8.17",
        "@types/react-dom": "^16.8.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
