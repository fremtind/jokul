{
    "name": "@fremtind/jkl-file-input-react",
    "version": "1.0.10",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react file-input",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/file-input-react",
    "keywords": [
        "jøkul",
        "fremtind"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "scripts": {
        "clean": "rimraf node_modules/ build/ node_modules/ ",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "node ./esbuild.prod.mjs",
        "build": "run-s build:*",
        "test": "jest --testMatch '**/file-input-react/**/*.test.+(ts|tsx|js)' --config=../../jest.config.js",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.dev.mjs",
        "dev:server": "nodemon ./dist/server.js",
        "dev": "node ./esbuild.dev.mjs && run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-button-react": "^13.0.13",
        "@fremtind/jkl-core": "^12.0.4",
        "@fremtind/jkl-file-input": "^1.0.1",
        "@fremtind/jkl-formatters-util": "^4.1.9",
        "@fremtind/jkl-icon-button-react": "^3.0.12",
        "@fremtind/jkl-icons-react": "^7.2.1",
        "@fremtind/jkl-input-group-react": "^2.1.3",
        "@fremtind/jkl-progress-bar-react": "^5.0.1",
        "@fremtind/jkl-react-hooks": "^11.2.0",
        "classnames": "^2.3.2"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
