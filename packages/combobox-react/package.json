{
    "name": "@fremtind/jkl-combobox-react",
    "version": "1.3.4",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react combobox",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/combobox-react",
    "keywords": [
        "jøkul",
        "fremtind"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "scripts": {
        "clean": "rimraf node_modules/ build/ node_modules/ ",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "node ./esbuild.prod.mjs",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.dev.mjs",
        "dev:server": "nodemon ./dist/server.js",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-combobox": "^1.2.1",
        "@fremtind/jkl-core": "^12.2.0",
        "@fremtind/jkl-icon-button-react": "^3.0.20",
        "@fremtind/jkl-icons-react": "^7.2.7",
        "@fremtind/jkl-input-group-react": "^2.1.10",
        "@fremtind/jkl-react-hooks": "^11.4.2",
        "@fremtind/jkl-select-react": "^13.1.4",
        "@fremtind/jkl-tag-react": "^4.0.22",
        "classnames": "^2.3.2"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
