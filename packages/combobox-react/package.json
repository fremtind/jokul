{
    "name": "@fremtind/jkl-combobox-react",
    "version": "2.3.75",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react combobox",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/combobox-react",
    "keywords": [
        "jøkul",
        "fremtind"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "scripts": {
        "clean": "git clean -xfd",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "node ./esbuild.prod.mjs",
        "build": "run-s build:*",
        "test": "jest --testMatch '**/combobox-react/**/*.test.+(ts|tsx|js)' --config=../../jest.config.js",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.dev.mjs",
        "dev:server": "nodemon ./dist/server.js",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-chip-react": "^1.0.3",
        "@fremtind/jkl-combobox": "^2.2.28",
        "@fremtind/jkl-core": "^14.8.6",
        "@fremtind/jkl-icon-button-react": "^5.0.27",
        "@fremtind/jkl-icons-react": "^9.1.19",
        "@fremtind/jkl-input-group-react": "^4.0.2",
        "@fremtind/jkl-react-hooks": "^12.2.10",
        "@fremtind/jkl-select-react": "^15.0.5",
        "@fremtind/jkl-tag-react": "^6.0.1",
        "@fremtind/jkl-tooltip-react": "^4.6.3",
        "classnames": "^2.3.2"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
