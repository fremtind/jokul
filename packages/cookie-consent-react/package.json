{
    "name": "@fremtind/jkl-cookie-consent-react",
    "version": "7.0.12",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react cookie consent component",
    "homepage": "https://github.com/fremtind/jokul/tree/main/packages/cookie-consent-react",
    "keywords": [
        "cookie consent",
        "jøkul",
        "fremtind"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "rimraf node_modules/ build/ node_modules/",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "node ./esbuild.prod.mjs",
        "build": "run-s build:*",
        "test": "jest --testMatch '**/cookie-consent-react/**/*.test.+(ts|tsx|js)' --config=../../jest.config.js",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.dev.mjs",
        "dev:server": "node ./dist/server.js",
        "dev": "node ./esbuild.dev.mjs && run-p dev:*"
    },
    "dependencies": {
        "@fremtind/jkl-button-react": "workspace:^12.0.11",
        "@fremtind/jkl-checkbox-react": "workspace:^8.0.8",
        "@fremtind/jkl-cookie-consent": "workspace:^9.0.6",
        "@fremtind/jkl-core": "workspace:^11.1.2",
        "@fremtind/jkl-list-react": "workspace:^8.0.7",
        "@fremtind/jkl-react-hooks": "workspace:^10.2.3",
        "react-a11y-dialog": "^6.2.0"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
