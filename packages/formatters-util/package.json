{
    "name": "@fremtind/jkl-formatters-util",
    "version": "2.2.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "Formatters for common data formats in Jøkul",
    "homepage": "https://jokul.fremtind.no",
    "keywords": [
        "jøkul",
        "fremtind"
    ],
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/browser/index.js",
    "scripts": {
        "clean": "rimraf .turbo/ build/ node_modules/",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "rollup --config ../../rollup.config.js",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:js": "ESBUILD_WATCH=true node ./esbuild.mjs",
        "dev:server": "node ./dist/server.js",
        "predev": "node ./esbuild.mjs",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@babel/runtime": "^7.17.8",
        "@fremtind/jkl-constants-util": "^0.4.4",
        "@fremtind/jkl-core": "^9.6.1"
    },
    "devDependencies": {
        "react-hook-form": "^7.29.0"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
