{
    "name": "@fremtind/jkl-text-input-react",
    "version": "3.8.12",
    "publishConfig": {
        "access": "public"
    },
    "description": "Jøkul react text input component",
    "keywords": [
        "text",
        "field",
        "textfield",
        "textarea",
        "input",
        "form",
        "jøkul",
        "fremtind"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/browser/index.js",
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "rimraf node_modules/ .cache/ build/ dist/ public/ **/*.css",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "rollup --config ../../rollup.config.js",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:style": "lerna exec --scope=@fremtind/jkl-text-input yarn build:watch",
        "dev:js": "parcel documentation/index.html",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@babel/runtime": "^7.9.0",
        "@fremtind/jkl-icon-button-react": "^0.4.10",
        "@fremtind/jkl-icons-react": "^1.4.2",
        "@fremtind/jkl-text-input": "^2.2.16",
        "@fremtind/jkl-typography-react": "^2.5.6",
        "classnames": "^2.2.6",
        "downshift": "^6.0.5",
        "match-sorter": "^5.0.0",
        "nanoid": "^3.1.22"
    },
    "devDependencies": {
        "@fremtind/jkl-portal-components": "^0.9.7",
        "@types/match-sorter": "^5.0.0"
    },
    "peerDependencies": {
        "@types/react": "^16.8.6 || ^17.0.0",
        "@types/react-dom": "^16.8.6 || ^17.0.0",
        "react": "^16.8.6 || ^17.0.0",
        "react-dom": "^16.8.6 || ^17.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
