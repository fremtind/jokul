{
    "name": "@fremtind/jkl-main-menu-react",
    "version": "0.0.1",
    "publishConfig": {
        "access": "public"
    },
    "description": "Hovedmenykomponent for Jøkul-løsninger",
    "homepage": "https://jokul.fremtind.no",
    "keywords": [
        "Menu",
        "Responsive",
        "Jøkul",
        "Fremtind"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/browser/index.js",
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "rimraf node_modules/ .cache/ build/ dist/ public/ **/*.css",
        "build:types": "tsc -p tsconfig-for-declarations.json",
        "build:scripts": "rollup --config ../../rollup.config.js",
        "build": "run-s build:*",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev:style": "lerna exec --scope=@fremtind/jkl-main-menu yarn build:watch",
        "dev:js": "parcel documentation/index.html",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "@babel/runtime": "^7.9.0",
        "classnames": "^2.2.6",
        "@fremtind/jkl-hamburger-react": "^1.4.4",
        "@fremtind/jkl-react-hooks": "^1.6.11"
    },
    "devDependencies": {
        "@nrk/core-toggle": "3.0.8"
    },
    "peerDependencies": {
        "@types/react": "^16.8.17",
        "@types/react-dom": "^16.8.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fremtind/jokul.git"
    },
    "bugs": {
        "url": "https://github.com/fremtind/jokul/issues"
    }
}
