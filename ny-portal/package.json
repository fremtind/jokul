        "@fremtind/browserslist-config-jkl": "^2.0.1",
        "@types/node": "^22.5.4",
        "@types/react": "^18.3.1",
        "@types/react-dom": "^18.3.1",
        "eslint": "^9.16.0",
        "eslint-config-next": "15.1.0",
        "prettier": "^3.4.2",
        "typescript": "^5.7.2"
    },
    "engines": {
        "node": ">=21.0.0"
    }
