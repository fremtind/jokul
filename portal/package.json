        "sanity": "^3.86.1",
        "sharp": "0.32.6",
        "slugify": "^1.6.6",
        "styled-components": "^6.1.17",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@types/node": "^22.14.1",
        "@types/react": "^18.3.20",
        "@types/react-dom": "^18.3.6",
