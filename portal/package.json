        "@sanity/image-url": "^1.1.0",
        "@sanity/locale-nb-no": "^1.1.19",
        "@sanity/ui": "^2.15.13",
        "@sanity/vision": "^3.86.1",
        "@types/mdx": "^2.0.13",
